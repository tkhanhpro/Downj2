{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/j2",
      "dest": "/api/j2"
    },
    {
      "src": "/api/down",
      "dest": "/api/down"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
